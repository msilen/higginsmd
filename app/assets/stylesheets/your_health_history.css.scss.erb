.green h3,h4{
  color:white;
  text-align:center;
}
.green.yhh{
  height:177px;
   padding-left: 94px;
   padding-right: 94px;
   h3{
     margin-top:14px;
     margin-bottom:13px;
   }
}
.yhhform{
  margin: 68px 0 0 90px;
  h4{
    color:black;
    text-align:left;
    display: block;
    margin-top:13px;
    font-size:10pt;
    line-height:19px;
  }
  .left_column{
    width:293px;
    margin:0;
    padding:0;
    .photo{
      float: right;
      margin-left:15px;
      margin-top: 13px;
    }
    #upload_photo{
      display:none;
    }
    .upload_button{
      text-decoration: none;
      -moz-box-shadow:    inset -1px -3px 10px #65b1ae;
      -webkit-box-shadow: inset -1px -3px 10px #65b1ae;
      box-shadow:         inset -1px -3px 10px #65b1ae;
      background: #82CECA;
      border: 1px solid #589B98;
      border-radius: 5px 5px 5px 5px;
      padding:10px;
      text-shadow: -1px -1px #68b1ad;
      font-size:13px;
      color:white;
      clear: both;
      display: block;
      margin-left: 163px;
      margin-top: 47px;
      margin-bottom:48px;
      width: 93px;
    }
  }
  label{
    color: #6faca7;
    font-family: HNCB;
    margin:0 0 5px 0;
    font-size:9pt;
  }
  textarea,input,select{
    margin: 7px -1px 0 0;
    width:100%;
    border:1px solid #cdcdcd;
    -moz-box-shadow:    inset 0px 1px 5px #f0f0f0;
    -webkit-box-shadow: inset 0px 1px 5px #f0f0f0;
    box-shadow:         inset 0px 1px 5px #f0f0f0;
    border-radius:5px;
  }
  .arrow{
    z-index:1000;
    position: relative;
    font-family:MPCI;
    margin: -8px 0 0 6px;
    color: #333333;
    padding:30px 0 0 46px;
    background: url('yhh_arrow_1.png') no-repeat;
  }
  input{
    width:323px;
    padding:9px;
    margin:2px 0 23px 0;
  }
  label{
    display:block;
  }
  .center_column{
    width:350px;
    margin:15px 0 0 25px;
    float: left;
  }
  select{
    background: white;
    border-radius: 4px 4px 4px 4px;
    border-style: solid;
    border-width: 1px; 
    width:342px;
    color: #6FACA7;
    margin-bottom: 22px;
    margin-top:0px;
    padding: 9px;
  }
  label.header_label{
    display:block !important;
  }
  .sex{
    width:100%;
    input{
      width: auto;
    }
    label.male{
      display: inline-block;
      padding: 5px 0 2px;
      width:51px;
      background: url('radio_button_bg.png') no-repeat 30px 0;
      margin-top:8px;
    }
    label.male.checked{
      background-position:30px -19px;
    }
    label.female{
      display: inline-block;
      padding: 5px 0 2px;
      width:70px;
      background: url('radio_button_bg.png') no-repeat 42px 0;
    }
    label.female.checked{
      background-position:42px -19px;
    }
    input[type=radio]{
      display:none;
    }
    .marital{
      float:right;
      width:217px;
      margin: -17px 0 0 0;
      select{
        width:209px;
      }
      label{
        float:left;
        width:100px;
      }
    }
  }
  .dob{
    select{
      width:104px;
      margin-right:3px;
    }
    select.year{
      width:116px;
    }
  }
}
